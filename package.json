{
  "name": "api-user",
  "version": "1.0.0",
  "description": "The User API for the Blog Portfolio with Apollo-GraphQL, Express and Mongoose",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node --presets @babel/preset-env",
