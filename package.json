    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.0.0",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
